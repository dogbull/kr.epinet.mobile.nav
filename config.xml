<?xml version='1.0' encoding='utf-8'?>
<widget id="kr.epinet.mobile.ansim2015" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ANSIM</name>
    <description>
        ANti-risk System of abrupt hazards as an Integrated Model
    </description>
    <author email="www.jrr.kr@gmail.com" href="http://jrr.kr">
        ParkJooHyeon
    </author>
    <content src="https://ansim2.epinet.kr" />
    <allow-navigation href="*" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
